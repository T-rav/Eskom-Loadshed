<?xml version='1.0' encoding='utf-8'?>
<widget id="com.stoneagetechnologies.saloadshed" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="PushAppsPlugin">
        <param name="android-package" value="com.pushapps.phonegap.PushAppsPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>SALoadshed</name>
    <description>
        An application to monitor Eskom's load shedding status and notify you.
    </description>
    <author email="mobilestoneagetechologies@gmail.com" href="http://www.stoneagetechnlogies.com/eskomloadshed">
        StoneAge Technologies
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
