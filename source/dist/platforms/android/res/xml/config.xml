<?xml version='1.0' encoding='utf-8'?>
<widget id="com.stoneagetechnologies.eskomloadshed" version="0.9.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="PushPlugin">
        <param name="android-package" value="com.plugin.gcm.PushPlugin" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <name>EskomLoadshed</name>
    <description>
        An application to monitor Eskom's load shedding status and notify you.
    </description>
    <author email="mobilestoneagetechologies@gmail.com" href="http://www.stoneagetechnlogies.com/eskomloadshed">
        StoneAge Technologies
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
